{
    "psi-header.variables": [
        [
            "author",
            "lg"
        ],
    ],
    "psi-header.lang-config": [
        {
            "language": ".mk",
            "begin": "",
            "prefix": "# ",
            "end": "",
            "blankLinesAfter": 0
        },
        {
            "language": ".s",
            "begin": "",
            "prefix": "; ",
            "end": "",
            "blankLinesAfter": 0
        },
    ],
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "Copyright (c) <<year>>, <<author>>",
                "",
                "SPDX-License-Identifier: BSD-2-Clause",
            ],
            "changeLogHeaderLineCount": 2,
        }
    ],
    "psi-header.changes-tracking.autoHeader": "autoSave",
    "C/C++ Include Guard.Prefix": "",
    "C/C++ Include Guard.Macro Type": "Filepath",
    "C/C++ Include Guard.Remove Extension": false,
    "C/C++ Include Guard.Path Depth": 2,
    "cSpell.words": [
        "Limine",
        "Pagemap"
    ],
}